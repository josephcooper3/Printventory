<h2>Inventory View</h2>
<table style="width:100%">
  <tr>
    <th>SKU</th>
    <th>Title</th>
    <th>Artist</th>
    <th>Sale Price</th>
    <th>Copies in Stock</th>
    <th></th>
  </tr>
  <% @products.each do |product| %>
  <tr>
    <td><%= product.id() %></td>
    <td><%= product.title() %></td>
    <td><%= product.artist_id() %></td>
    <td><%= product.sale_price() %></td>
    <td><%= product.copies_in_stock() %></td>
    <td><a href="/products/<%= product.id() %>">Full Details</a></td>
  </tr>
  <% end %>
</table>
